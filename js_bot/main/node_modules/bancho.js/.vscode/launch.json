{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}",
			"protocol": "inspector"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Tests",
			"program": "${workspaceRoot}/tests/run.js"
		}
	]
}