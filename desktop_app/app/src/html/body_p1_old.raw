                <div class="user-welcome">
                    <div class="avatar" ><img src="{props.avatar_url}" id="userAvatar"/></div>
                    <div class="user_informations">
                        <span id="welcomePseudo">{props.username}</span>
                        <span id="welcomeRank">Rank: {props.global_rank} ({props.country_rank} {props.country})</span>
                        <span id="welcomePP">{props.pp}PP</span>
                    </div>
                </div>
    
                <div class="user-stats">
                    <!-- <div class="stat-container">
                        <span  class="stat-label">Classed Score</span>
                        <span class="stat-info" id="userStat_classedScore"></span></span>
                    </div> -->

                    <div class="stat-container">
                        <span class="stat-label">$: ('{this.props.lang}' === 'fr') ? 'Précision' : 'Accuracy' $</span>
                        <span class="stat-info" id="userStat_Accuracy" >{props.accuracy} %</span>
                    </div>

                    <div class="stat-container">
                        <span  class="stat-label">$: ('{this.props.lang}' === 'fr') ? 'Parties' : 'Plays' $</span>
                        <span class="stat-info" id="userStat_classedPlays">{props.playcount}</span>
                    </div>

                    <div class="stat-container">
                        <span  class="stat-label">$: ('{this.props.lang}' === 'fr') ? 'Score Total' : 'Total Score' $</span>
                        <span class="stat-info" id="userStat_TotalScore">{props.total_score}</span>
                    </div>

                    <div class="stat-container">
                        <span  class="stat-label">Clicks</span>
                        <span class="stat-info" id="userStat_nbClick">{props.clicks}</span>
                    </div>

                    <div class="stat-container">
                        <span id="userStat_classedScore" class="stat-label">$: ('{this.props.lang}' === 'fr') ? 'Combo Max' : 'Max Combo' $</span>
                        <span class="stat-info" id="userStat_maxCombo">{props.maxCombo}</span>
                    </div>

                    <div class="rank-highligh" id="rank_highligh"><span class="loading_holder t100p h90 left right"></div>

                    <div class="rank-notations">
                        <div class="rank-note">
                            <label>SSH</label>
                            <span id="userStat_nb_ssh">{props.ssh}</span>
                        </div>

                        <div class="rank-note">
                            <label>SS</label>
                            <span id="userStat_nb_ss">{props.ss}</span>
                        </div>

                        <div class="rank-note">
                            <label>SH</label>
                            <span id="userStat_nb_sh">{props.sh}</span>
                        </div>

                        <div class="rank-note">
                            <label>S</label>
                            <span id="userStat_nb_s">{props.s}</span>
                        </div>

                        <div class="rank-note">
                            <label>A</label>
                            <span id="userStat_nb_a">{props.a}</span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="home_user_profile">
                <div class="title-categories">$: ('{this.props.lang}' === 'fr') ? 'Tes meilleurs scores' : 'Your Best Scores' $</div>
                <div id="best_scores" class="scores_list">

 