 <div class="windows-nav">{props.windowTitle}</div>
    <div class="nav-top">
        <div class="search-tools">
            <div class="search-bar">$: ('{this.props.lang}' === 'fr') ? 'Rechercher..' : 'Search..' $</div>
            <div class="search-btn"><ion-icon name="search-outline"></ion-icon></div>
        </div>
        <div class="users-tools">
            <div class="nav-btn-action"><ion-icon name="person-outline"></ion-icon></div>
            <div class="nav-btn-action"><ion-icon name="notifications-outline"></ion-icon></div>
            <div class="nav-btn-action"><ion-icon name="settings-outline"></ion-icon></div>
        </div>
        <div class="open-boxedit reply">
            <div class="boxContainer">
                <div class="settings-container">
                    <div class="setup-handler">
                        <label>$: ('{this.props.lang}' === 'fr') ? 'Raffrechissement' : 'Refresh Rate' $</label>
                        <div class="btn-row">
                          <div class="rr-btn toggle">$: ('{this.props.lang}' === 'fr') ? 'Bas' : 'Low' $</div>
                          <div class="rr-btn">$: ('{this.props.lang}' === 'fr') ? 'Modéré' : 'Moderate' $</div>
                          <div class="rr-btn">$: ('{this.props.lang}' === 'fr') ? 'Haut' : 'High' $</div>
                          <div class="rr-btn">$: ('{this.props.lang}' === 'fr') ? 'Extrême' : 'Extrem' $</div>
                        </div>
                    </div>
                    <div class="setup-handler">
                        <label>Notifications</label>
                        <div>
                            <div class="slide-btn">
                                <div class="mod-btn">
                                    <div class="cursor">On</div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="setup-handler">
                        <label>$: ('{this.props.lang}' === 'fr') ? 'MàJ des scores au lancement' : 'Update scores when Launch' $</label>
                        <div>
                            <div class="slide-btn">
                                <div class="mod-btn">
                                    <div class="cursor">On</div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="setup-handler">
                        <label>$: ('{this.props.lang}' === 'fr') ? 'MàJ DB au lancement' : 'Update BM db when Launch' $</label>
                        <div>
                            <div class="slide-btn">
                                <div class="mod-btn">
                                    <div class="cursor">On</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="soft_infos">
                <span id="software_version"><span class="loading_holder t3 h6 "></span></span>
                <p>&nbsp;|&nbsp;</p>
                <span id="application_id"><span class="loading_holder t3 h6 "></span></span>
            </div>
        </div>
    </div>
    <div class="global-contents"> 
        <div class="main-content active" id="HomePage">
