{
  "name": "bellafiora",
  "version": "1.0.920",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win ",
    "build": "pkg main.js --target win --output bellafiora --node 16"
  },
  "license": "MIT",
  "author": "",
  "protocols": {
    "myapp": {
      "schemes": [
        "app"
      ]
    }
  },
  "dependencies": {
    "axios": "^1.6.7",
    "child_process": "^1.0.2",
    "credentials": "file:../../credentials",
    "crypto": "^1.0.1",
    "dns": "^0.2.2",
    "dotenv": "^16.4.1",
    "electron-remote": "^1.3.0",
    "electron-shell": "^0.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "highcharts": "^11.3.0",
    "http": "^0.0.1-security",
    "ini": "^4.1.1",
    "long": "^5.2.3",
    "os": "^0.1.2",
    "osu-bpdpc": "^0.3.0",
    "osudb": "^2.0.0",
    "path": "^0.12.7",
    "remote-server": "file:../../remote-server",
    "tmp": "^0.2.1",
    "uleb128": "^1.0.1",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "vm": "^0.1.0",
    "websocket": "^1.0.34",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "clean-modules": "^3.0.4",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1"
  },
  "win": {
    "target": "nsis"
  },
  "files": [
    "**/*"
  ],
  "build": {
    "appId": "fr.techalchemy.bellafiora",
    "productName": "Bella Fiora test",
    "compression": "maximum",
    "win": {
      "target": "nsis",
      "asar": true,
      "artifactName": "${productName} Setup v${version}.${ext}",
      "extraFiles": [
        {
          "from": "./misc/software/gosumemory.exe",
          "to": ".",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "./misc/software/config.ini",
          "to": ".",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "./build/presence.conf",
          "to": ".",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "nsis": {
      "license": "LICENSE.txt",
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
